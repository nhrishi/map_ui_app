{
  "name": "myfirst-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node ./server.js",
    "server": "nodemon ./server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\""
  },
  "author": "Hrishikesh Nashikkar",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fabric-ca-client": "^1.4.4",
    "fabric-client": "^1.4.4",
    "grpc": "^1.22.2",
    "log4js": "^4.5.1",
    "material-ui": "^0.20.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
